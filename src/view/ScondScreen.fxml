            <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="1120.0" spacing="40.0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
