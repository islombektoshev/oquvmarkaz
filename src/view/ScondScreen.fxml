<VBox prefHeight="781.0" prefWidth="1120.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScondScreenController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
